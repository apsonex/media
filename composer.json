{
  "name": "apsonex/media",
  "authors": [
    {
      "name": "Apsonex Inc.",
      "email": "apsonexinc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "enshrined/svg-sanitize": "^0.15.0",
    "intervention/image": "^3.2",
    "spatie/laravel-image-optimizer": "^1.7",
    "spatie/temporary-directory": "^2.2"
  },
  "require-dev": {
    "laravel/framework": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Apsonex\\Media\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Apsonex\\Media\\Tests\\": "tests"
    }
  },
  "scripts": {},
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Apsonex\\Media\\MediaServiceProvider"
      ],
      "aliases": {
        "Mdeia": "Apsonex\\Media\\Facades\\Media"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
