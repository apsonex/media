{
  "name": "apsonex/media",
  "authors": [
    {
      "name": "Apsonex Inc.",
      "email": "apsonexinc@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "enshrined/svg-sanitize": "^0.15.4",
    "illuminate/support": "^9.6",
    "intervention/image": "^2.7",
    "spatie/laravel-image-optimizer": "^1.7",
    "spatie/temporary-directory": "^2.1"
  },
  "require-dev": {
    "fakerphp/faker": "^1.19",
    "nunomaduro/collision": "^6.1",
    "apsonex/mls-data": "dev-main",
    "orchestra/testbench": "^7.1",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Apsonex\\Media\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Apsonex\\Media\\Tests\\": "tests"
    }
  },
  "scripts": {},
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Apsonex\\Media\\MediaServiceProvider"
      ],
      "aliases": {
        "Mdeia": "Apsonex\\Media\\Facades\\Media"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:apsonex/mls-data.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:apsonex/rets.git"
    }
  ]
}
